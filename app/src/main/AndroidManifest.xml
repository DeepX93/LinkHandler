<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.linkhero.linkhandler">

    <application
        android:allowBackup="true"
        android:theme="@style/Theme.AppCompat.Light">

        <!-- Background Service to Handle Links -->
        <service android:name=".LinkInterceptorService"
            android:exported="false" />

        <!-- Content Provider for Plugin Communication -->
        <provider
            android:name=".PluginProvider"
            android:authorities="com.linkhero.linkhandler.provider"
            android:exported="true" />

    </application>

</manifest>
